                "conversionFactorDeliveryUnitStockUnit": {
                  "type": "number",
                  "format": "decimal",
                  "minimum": 0,
                  "maximum": 9999999.999999,
                  "description": "Coefficient delivery unit -> stock unit.",
                  "example": 1
                },
                "conversionFactorDeliveryUnitPriceUnit": {
                  "type": "number",
                  "format": "decimal",
                  "minimum": 0,
                  "maximum": 9999999.999999,
                  "description": "Coefficient delivery unit -> price unit.",
                  "example": 2
                },
                "conversionFactorPriceUnitDeliveryUnit": {
                  "type": "number",
                  "format": "decimal",
                  "minimum": 0,
                  "maximum": 9999999.999999,
                  "description": "Coefficient price unit -> delivery unit.",
                  "example": 0.5
                },
                "orderQuantityStockUnit": {
                  "type": "number",
                  "format": "decimal",
                  "minimum": -9999999999.9999,
                  "maximum": 9999999999.9999,
                  "description": "Order quantity in stock units.",
                  "example": 1000
                },
                "orderQuantityDeliverUnit": {
                  "type": "number",
                  "format": "decimal",
                  "minimum": -9999999999.9999,
                  "maximum": 9999999999.9999,
                  "description": "Order quantity in delivery units.",
                  "example": 100
                },
                "orderQuantityPriceUnit": {
                  "type": "number",
                  "format": "decimal",
                  "minimum": -9999999999.9999,
                  "maximum": 9999999999.9999,
                  "description": "Order quantity in price units.",
                  "example": 200
                },
                "deliveryUnitCode": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 280001,
                  "maximum": 289999,
                  "description": "C28 - Delivery unit code.",
                  "example": 280005
                },
                "priceUnitCode": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 280001,
                  "maximum": 289999,
                  "description": "C28 - Price unit code.",
                  "example": 280002
                },
                "priceTypeCode": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 24580001,
