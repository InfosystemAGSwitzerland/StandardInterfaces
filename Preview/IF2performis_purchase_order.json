{
  "openapi": "3.0.1",
  "info": {
    "title": "IF2performis",
    "version": "1.0.0",
    "contact": {
      "name": "Infosystem AG",
      "email": "support@infosystem.ch",
      "url": "https://www.infosystem.ch"
    },
    "description": "IF2performis Rest API"
  },
  "servers": [],
  "paths": {
    "/api/v1/purchase/orders": {
      "get": {
        "summary": "Get purchase order information",
        "tags": [
          "Purchase"
        ],
        "responses": {
          "200": {
            "description": "Purchase order information.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/purchaseOrder"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Invalid input, object invalid",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/problemDetails"
                }
              }
            }
          },
          "401": {
            "description": "Basic Authentication failed"
          },
          "403": {
            "description": "Requestor has insufficient rights"
          },
          "500": {
            "description": "Internal Server Error"
          },
          "503": {
            "description": "Service unavailable due to maintenance",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/maintenanceWindow"
                }
              }
            }
          }
        },
        "operationId": "getPurchaseOrders",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "maxLength": 50
            },
            "in": "header",
            "name": "X-Requesting-System",
            "description": "Identifying the system calling the service.",
            "required": true
          },
          {
            "schema": {
              "type": "string",
              "maxLength": 30
            },
            "in": "header",
            "name": "X-Requesting-User",
            "description": "Identifying the user calling the service.",
            "required": true
          },
          {
            "schema": {
              "type": "string",
              "maxLength": 30
            },
            "in": "header",
            "name": "X-Flow-ID",
            "description": "Optional technical id (GUID) for call identification."
          },
          {
            "schema": {
              "type": "string",
              "maxLength": 30
            },
            "in": "header",
            "name": "X-Client-Type",
            "description": "Optional client type calling the service."
          },
          {
            "schema": {
              "type": "integer",
              "format": "int32",
              "minimum": 0,
              "maximum": 100000
            },
            "in": "query",
            "name": "skip",
            "description": "Number of entries to skip for pagination."
          },
          {
            "schema": {
              "type": "integer",
              "format": "int32",
              "minimum": 0,
              "maximum": 1000,
              "default": 100
            },
            "in": "query",
            "name": "limit",
            "description": "Maximum number of entries to return."
          },
          {
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "in": "query",
            "name": "changesSinceDateTime",
            "description": "Get all entries which were added or edited since the date time specified."
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "query",
            "name": "changesSinceChangeId",
            "description": "Get all entries which were added or edited since the change id specified."
          },
          {
            "schema": {
              "type": "integer",
              "format": "int64",
              "minimum": 1,
              "maximum": 9999999999
            },
            "in": "query",
            "name": "purchaseOrderId",
            "description": "Get the entry with the specified purchase order id."
          },
          {
            "schema": {
              "type": "string",
              "example": "ISBRW",
              "maxLength": 200
            },
            "in": "query",
            "name": "origin",
            "description": "Get the entries with the specified origin."
          },
          {
            "schema": {
              "type": "string",
              "example": "C47C97C0F4B04096855955CB5AD47444",
              "maxLength": 200
            },
            "in": "query",
            "name": "externalPurchaseOrderId",
            "description": "Get the entries with the specified external purchase order id - should be used with an origin."
          },
          {
            "schema": {
              "type": "string",
              "maxLength": 20,
              "example": "15523"
            },
            "in": "query",
            "name": "purchaseOrderNo",
            "description": "Get the entries with the specified purchase order number - only unique with company number."
          },
          {
            "schema": {
              "type": "integer",
              "format": "int64",
              "minimum": 1,
              "maximum": 999999
            },
            "in": "query",
            "name": "companyNo",
            "description": "Get the entries with the specified company number."
          },
          {
            "schema": {
              "type": "boolean"
            },
            "in": "query",
            "name": "includeDetails",
            "description": "Defines if order positions are included in the response."
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "purchaseOrder": {
        "type": "object",
        "title": "PurchaseOrder",
        "description": "Represents a purchase order entry.",
        "properties": {
          "changeId": {
            "type": "integer",
            "description": "Change identification of the entry.",
            "format": "int64",
            "readOnly": true
          },
          "changeDate": {
            "type": "string",
            "description": "Change date of the entry.",
            "format": "date-time",
            "example": "2019-08-24T14:15:22Z",
            "readOnly": true
          },
          "modifyUser": {
            "type": "string",
            "maxLength": 30,
            "minLength": 1,
            "description": "User who last modified the entry.",
            "example": "STJ",
            "readOnly": true
          },
          "modifyDate": {
            "type": "string",
            "format": "date-time",
            "description": "Last modification date of the entry.",
            "example": "2024-01-30T11:46:59",
            "readOnly": true
          },
          "purchaseOrderId": {
            "type": "integer",
            "format": "int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Technical identification of the entry.",
            "example": 322784
          },
          "companyNo": {
            "type": "integer",
            "format": "int64",
            "minimum": 0,
            "maximum": 999999,
            "description": "Company identification of the entry.",
            "example": 100
          },
          "purchaseOrderNo": {
            "type": "string",
            "minLength": 1,
            "maxLength": 20,
            "description": "Purchase order number of the entry.",
            "example": "B24.1234"
          },
          "externalPurchseOrderId": {
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "description": "External id of the entry - may be used instead of the technical identification.",
            "example": "S-35-50932"
          },
          "origin": {
            "type": "string",
            "maxLength": 200,
            "minLength": 1,
            "description": "Origin of the entry.",
            "example": "MYERP",
            "readOnly": true
          },
          "supplierID": {
            "type": "integer",
            "format": "int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Id of the supplier of the entry.",
            "example": 322784
          },
          "purchaseOrderTypeCode": {
            "type": "integer",
            "format": "int64",
            "minimum": 24620001,
            "maximum": 24629999,
            "description": "C2462 - How was the order placed? Post, eMail,...",
            "example": 24620001
          },
          "purchaseOrderStateCode": {
            "type": "integer",
            "format": "int64",
            "minimum": 24520001,
            "maximum": 24529999,
            "description": "C2452 - State of the order e.g. recorded, placed, partially delivered,...",
            "example": 24520002
          },
          "currency": {
            "type": "string",
            "maxLength": 3,
            "description": "Currency ISO code.",
            "example": "CHF"
          },
          "exchangeRate": {
            "type": "number",
            "format": "double",
            "minimum": 0.000001,
            "maximum": 99999999.999999,
            "description": "Exchange rate used for purchase when not in own currency.",
            "example": 0.954
          },
          "isPurchaseOrderSent": {
            "type": "boolean",
            "description": "Flag if the purchase order was been sent."
          },
          "purchaseOrderSendDate": {
            "type": "string",
            "format": "datetime",
            "description": "Date the purchase order was sent.",
            "example": "2024-03-01"
          },
          "deliveryDateRequested": {
            "type": "string",
            "format": "datetime",
            "description": "Requested delivery date, if no confirmation, this is written to the delivery date.",
            "example": "2024-03-15"
          },
          "deliveryDate": {
            "type": "string",
            "format": "datetime",
            "description": "Confirmed delivery date. Equals requested delivery date if not confirmed.",
            "example": "2024-03-17"
          },
          "priceDate": {
            "type": "string",
            "format": "datetime",
            "description": "Date of the price information the purchase is based on.",
            "example": "2024-02-25"
          },
          "orderConfirmationDateRequested": {
            "type": "string",
            "format": "datetime",
            "description": "Date the supplier should confirm the order.",
            "example": "2024-01-05"
          },
          "orderConfirmationDate": {
            "type": "string",
            "format": "datetime",
            "description": "Date the order was confirmed.",
            "example": "2024-01-05"
          },
          "orderConfirmationNo": {
            "type": "string",
            "maxLength": 40,
            "description": "Order confirmation no from the supplier.",
            "example": "A18.223232"
          },
          "storageId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 0,
            "maximum": 9999999999,
            "description": "Identifier of the storage.",
            "example": 2315
          },
          "storageAreaId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 0,
            "maximum": 9999999999,
            "description": "Identifier of the storage area.",
            "example": 231
          },
          "shippingAddressCode": {
            "type": "integer",
            "format": "int64",
            "minimum": 24640001,
            "maximum": 24649999,
            "description": "C2464 - Where is the purchase delivered? (24640001 - pickup address, 24640002 - shipping address)",
            "example": 24640002
          },
          "shippingBusinessPartnerAdresssId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Unique identifier of the shipping business partner address (Requried for shippings).",
            "example": 3215
          },
          "pickupBusinessPartnerAdresssId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Unique identifier of the pickup business partner address (Requried for pickups).",
            "example": 3216
          },
          "pickupBusinessPartnerId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Unique identifier of the contact person at the pickup address.",
            "example": 2345
          },
          "shippingTypeCode": {
            "type": "integer",
            "format": "int64",
            "minimum": 24630001,
            "maximum": 24639999,
            "description": "C2463 - How is the purchase shipped? Pick up, lorry, post,...",
            "example": 24630010
          },
          "incoTermsCode": {
            "type": "integer",
            "format": "int64",
            "minimum": 24670001,
            "maximum": 24679999,
            "description": "C2467 - Incoterms define the responsibilities of exporters and importers in the arrangement of shipments and the transfer of liability involved at various stages of the transaction.",
            "example": 24670002
          },
          "incoTermsLocation": {
            "type": "string",
            "maxLength": 50,
            "description": "Location for the incoterms.",
            "example": "ZÃ¼rich"
          },
          "destinationDeliveryDateRequested": {
            "type": "string",
            "format": "datetime",
            "description": "Requested destination delivery date, e.g. date of transfer of responsibility.",
            "example": "2024-05-06"
          },
          "destinationDeliveryDate": {
            "type": "string",
            "format": "datetime",
            "description": "Destination delivery date, e.g. date of transfer of responsibility.",
            "example": "2024-05-05"
          },
          "ordererBusinessPartnerAdresssId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Unique identifier of the business partner address of the orderer.",
            "example": 1394
          },
          "ordererBusinessPartnerId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Unique identifier of the orderer.",
            "example": 3264
          },
          "supplierBusinessPartnerAdresssId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Unique identifier of the supplier business partner address.",
            "example": 3215
          },
          "supplierBusinessPartnerId": {
            "type": "integer",
            "format": "Int64",
            "minimum": 1,
            "maximum": 9999999999,
            "description": "Unique identifier of the contact person at the supplier.",
            "example": 2345
          },
          "supplierNotes": {
            "type": "string",
            "maxLength": 2000,
            "description": "Supplier related notes and remarks",
            "example": "Every year in early summer, the supplier runs a promotion on all metal goods. Internal task recorded in the info centre so that we can place a bulk order for screws and fasteners."
          },
          "internalRemarks": {
            "type": "string",
            "maxLength": 2000,
            "description": "Internal remarks about the supplier.",
            "example": "Promised delivery dates are never met."
          },
          "externalRemarks": {
            "type": "string",
            "maxLength": 2000,
            "description": "Remarks added to the supplier order.",
            "example": "Please let us know the delivery dates in more detail."
          },
          "openingText": {
            "type": "string",
            "maxLength": 4000,
            "description": "Text opening the supplier order.",
            "example": "You have received the following order. Please check the items and confirm the delivery by means of an order confirmation."
          },
          "footerText": {
            "type": "string",
            "maxLength": 4000,
            "description": "Final remarks concerning the order put in the footer of the order.",
            "example": "Please inform us of any price changes in writing at the e-mail address purchase@memor.ch. Thank you very much."
          },
          "attributeValueSet": {
            "$ref": "#/components/schemas/attributeValueSet"
          },
          "materialRequirementsPlanningGroupCode": {
            "type": "integer",
            "format": "int64",
            "minimum": 4020001,
            "maximum": 4029999,
            "description": "C402 - Material requirements planning (MRP) group.",
            "example": 4020005
          },
          "reference": {
            "type": "string",
            "maxLength": 100,
            "description": "Reference that helps reidentify the purchase.",
            "example": "E24-26873-F3"
          },
          "originOrderNo": {
            "type": "string",
            "maxLength": 20,
            "description": "Order number of the associated sales or production order.",
            "example": "V24.1234"
          },
          "automaticCompletion": {
            "type": "boolean",
            "description": "Is the purchase order automatically completed once it has been fully delivered and invoiced?"
          },
          "positions": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "positionId": {
                  "type": "integer",
                  "format": "Int64",
                  "minimum": 1,
                  "maximum": 9999999999,
                  "description": "Unique id of the order position.",
                  "example": 123456
                },
                "positionNr": {
                  "type": "number",
                  "format": "decimal",
                  "minimum": 0,
                  "maximum": 9999.999,
                  "description": "Item number. Format and conditions via company settings.",
                  "example": 2.2
                },
                "modifyUser": {
                  "type": "string",
                  "maxLength": 30,
                  "minLength": 1,
                  "description": "User who last modified the entry.",
                  "example": "STJ"
                },
                "modifyDate": {
                  "type": "string",
                  "format": "date-time",
                  "description": "Date the entry was last modified",
                  "example": "2024-01-30T11:46:59"
                },
                "positionTypeCode": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 24550001,
                  "maximum": 24559999,
                  "description": "C2455 - Type of the position\n1: article\n2: amount\n3: text.",
                  "example": 24550003
                },
                "articleId": {
                  "type": "integer",
                  "format": "Int64",
                  "minimum": 1,
                  "maximum": 9999999999,
                  "description": "Unique identifier of the article.",
                  "example": 365972
                },
                "text": {
                  "type": "string",
                  "maxLength": 2000,
                  "description": "Text of the position.",
                  "example": "WD-40"
                },
                "conversionFactorStockUnitDeliveryUnit": {
                  "type": "number",
                  "format": "decimal",
