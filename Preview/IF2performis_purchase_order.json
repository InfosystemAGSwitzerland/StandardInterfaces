                  "example": 5026.73
                },
                "storageId": {
                  "type": "integer",
                  "format": "Int64",
                  "minimum": 0,
                  "maximum": 9999999999,
                  "description": "Identifier of the storage - Overrides values in header.",
                  "example": 2315
                },
                "storageAreaId": {
                  "type": "integer",
                  "format": "Int64",
                  "minimum": 0,
                  "maximum": 9999999999,
                  "description": "Identifier of the storage location - Overrides values in header.",
                  "example": 231
                },
                "shippingTypeCode": {
                  "type": "integer",
                  "format": "int64",
                  "minimum": 24630001,
