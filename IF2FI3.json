            }
          },
          {
            "name": "activeOnly",
            "in": "query",
            "description": "If set to true, only the active sales channels will be returned.",
            "schema": {
              "type": "boolean"
            }
          },
          {
            "name": "X-RequestingSystem",
            "in": "header",
            "description": "Identifing the system calling the service.",
